@article{specoviusneugebauer1990,
author="Specovius-Neugebauer, Maria",
title="{The Helmholtz decompostion of weighted $L\sp r$-spaces.}",
language="English",
journal="Commun. Partial Differ. Equations ",
volume="15",
number="3",
pages="273-288",
year="1990",
doi={10.1080/03605309908820687},
abstract="{This paper deals with the decomposition of vector fields into
    solenoidal vector fields and gradient fields. This decomposition is
    important in the theory of fluid mechanics and electromagnetism. \par
    Consider a vector field defined on an exterior domain $\Omega \subset
    {\bbfR}\sp n$ with $3\le n$. The author studies the decomposition of such a
    vector field which is in a weighted $L\sp p$ space into a solenoidal
    (divergence-free) vector field and an irrotational (curl-free) vector
    field.}",
reviewer="{A.J.Meir}",
keywords="{decomposition of vector fields into solenoidal vector fields and
    gradient fields; fluid mechanics; electromagnetism; weighted $L\sp p$ space;
    divergence-free; curl-free}",
