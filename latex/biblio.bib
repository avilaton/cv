@article{Andersson1989,
author="Andersson, Lars",
title="{On the space of asymptotically Euclidean metrics.}",
language="English",
journal="Compos. Math. ",
volume="69",
number="1",
pages="61-81",
year="1989",
abstract="{The paper aims to extend the theory of the space of Riemannian
    metrics on a compact manifold (due, in particular, to Ebin and Bourguignon)
    to the case of asymptotically Euclidean metrics on ${\bbfR}\sp n$. The main
    result of the paper is a slice theorem for the action of the group ${\frak
    D}$ of asymptotically Euclidean diffeomorphisms of ${\bbfR}\sp n$ on the
    space ${\frak M}$ of all such metrics. This and other results of the paper
    should serve as a foundation for the analysis of the factor space ${\frak
    M}/{\frak D}$. The details of this analysis are postponed to a future
    paper.}",
reviewer="{N.V.Ivanov}",
keywords="{space of metrics; group of diffeomorphisms; asymptotically Euclidean
    metrics}",
classmath="{*58D17 (Manifolds of metrics)}",
}

